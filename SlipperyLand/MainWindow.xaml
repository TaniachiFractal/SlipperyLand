<Window x:Class="SlipperyLand.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SlipperyLand"
        xmlns:viewmodel="clr-namespace:ViewModel;assembly=ViewModel"
        d:DataContext="{d:DesignInstance Type=viewmodel:MainWindowViewModel}"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        FontFamily="Cascadia Mono"
        Title="MainWindow" Width="512" Height="512">
    <Grid>

        <TextBlock Text="{Binding MapText}" />
        <Button Command="{Binding ChangeRandomCellCommand}" Width="50"  Height="50"/>

    </Grid>
</Window>
